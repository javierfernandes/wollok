                 commandId="org.uqbar.project.wollok.WollokDsl.validate"
                 style="push"
                 tooltip="%validate.description">
            <visibleWhen checkEnabled="false">
                <reference
                    definitionId="org.uqbar.project.wollok.WollokDsl.Editor.opened">
                </reference>
            </visibleWhen>
         </command>  
         </menuContribution>
