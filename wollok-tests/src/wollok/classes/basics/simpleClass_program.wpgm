import simpleClass.Golondrina

program simpleClass {
	
	const pepita = new Golondrina()

	var r = pepita.getEnergia() == 50
	assert.that(r)
	
	//// una segunda
	//
	const pepona = new Golondrina()
	pepona.volar(2)
	assert.equals(48, pepona.getEnergia())
	assert.equals(50, pepita.getEnergia())  // pepita no se altero
	//
	//

}