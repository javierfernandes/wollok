/* XPECT_SETUP org.uqbar.project.wollok.tests.typesystem.xpect.TypeSystemXpectTestCase END_SETUP */

class Golondrina { 
	method vola(kilometros) { }
	
	method come(grms) { }
}

class Entrenador {
	// XPECT methodType at falla --> (Golondrina) => Any
	method falla(ave) { 
		// XPECT! warnings --> "Golondrina does not understand habla()" at "ave.habla()"
		ave.habla()
		
		ave.vola(1)
		
		// XPECT warnings --> "Golondrina does not understand come(). However other methods exist with different argument count: come(grms)" at "ave.come()" 
		return ave.come()	
	}	
}
