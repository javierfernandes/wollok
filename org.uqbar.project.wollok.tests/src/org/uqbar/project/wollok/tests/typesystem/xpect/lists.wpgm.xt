/* XPECT_SETUP org.uqbar.project.wollok.tests.typesystem.xpect.TypeSystemXpectTestCase END_SETUP */

program lists {
	
	// XPECT type at cfirst --> Number
	const cfirst = [1, 2, 3].first() 

	// XPECT type at asList --> List
	const asList = [1, 2, 3].asList()
	
	// XPECT type at asSet --> Set
	const asSet = [1, 2, 3].asSet()
	
	// XPECT type at isEmpty --> Boolean
	const isEmpty = [1, 2, 3].isEmpty()
	
	// XPECT type at contains --> {(Number) => Boolean}
	const contains = { e => [1, 2, 3].contains(e) }

	// XPECT type at occurrencesOf --> {(Number) => Number}
	const occurrencesOf = { e => [1, 2, 3].occurrencesOf(e) }
	
}
