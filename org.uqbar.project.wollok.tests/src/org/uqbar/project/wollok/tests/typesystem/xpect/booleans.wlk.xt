/* XPECT_SETUP org.uqbar.project.wollok.tests.typesystem.xpect.TypeSystemXpectTestCase END_SETUP */

object booleanConditionError {
	const a = 2
	const b = 3
	
	method m() {
		return rules.a()
	}

	// XPECT methodType at m1 --> () => Boolean
	method m1() {
		return rules.a()
	}
	
	// XPECT methodType at m2 --> () => Boolean
	method m2() = a > 0 && b < 0

	// XPECT methodType at m3 --> () => Boolean
	method m3() = a < 0 || b > 0

	// XPECT methodType at m4 --> () => Boolean
	method m4() = (a > 0) and (b < 0)

	// XPECT methodType at m5 --> () => Boolean
	method m5() {
		return (a < 0) or (b > 0)
	}
	
	// XPECT methodType at m6 --> () => Boolean
	method m6() = (a > 0).negate()

	// XPECT methodType at m7 --> () => Boolean
	method m7() = !(a > 0)

	// XPECT methodType at m8 --> () => Boolean
	method m8() = not (a > 0)
	
	// XPECT methodType at m9 --> () => String
	method m9() = rules.a().toString()
	
	// XPECT methodType at m10 --> (Boolean) => Boolean
	method m10(b) = !b
}

object rules {
	// XPECT methodType at a --> () => Boolean
	method a() = true
}
