/* XPECT_SETUP org.uqbar.project.wollok.tests.xpect.WollokXPectTest END_SETUP */
class X {
	const x
	
	method x() = x
}

describe "group of tests" {
	const a
	var b
	const c = 4
	
	fixture {
		// XPECT errors --> "Cannot modify constants" at "a"
		a = new X(x = 10)
		// XPECT errors --> "Cannot modify constants" at "a"
		a = 8
		b = 1
		b = 3
		c = 55
	}
	
	test "easy test" {
		assert.equals(8, a)
	}
	
}