/* XPECT_SETUP org.uqbar.project.wollok.tests.xpect.WollokXPectTest END_SETUP */

class Foo {
	// Allowed
    override method == (other) {
        return true
    }
}

object foo {
	// Allowed
    override method == (other) {
        return true
    }
}

class Bar {
	// XPECT warnings --> "Method must be marked as override, since it overrides a superclass method" at "=="
    method == (other) {
        return true
    }

	// XPECT errors --> "You can't use override here: method doesn't exist in superclass and you can't change superclass definition" at "fafafa"
	override method fafafa() {}
}

object bar {
	// XPECT warnings --> "Method must be marked as override, since it overrides a superclass method" at "=="
    method == (other) {
        return true
    }

	// XPECT errors --> "You can't use override here: method doesn't exist in superclass and you can't change superclass definition" at "fafafa"
	override method fafafa() {}
}
