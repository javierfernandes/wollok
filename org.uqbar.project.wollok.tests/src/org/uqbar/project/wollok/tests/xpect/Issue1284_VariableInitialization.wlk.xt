class Producto {
	var precioRegular
	
	constructor() {
		self.initialize()
	}
	
	constructor(valor) {
		self.determinarPrecio(valor)	
	}
	
	constructor(valor1, valor2) {
		self.precioRegular(valor1 + valor2)	
	}
	
 	override method initialize() {
		self.initialize(0)
	}

 	method initialize(valor) {
		precioRegular = 0	
	}
	
	method determinarPrecio(valor) {
		precioRegular = 100 + valor
	}
	
	method precioRegular(precio) { precioRegular = precio }
}