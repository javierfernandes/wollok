import wollok.lib.assert

test "testWithBinaryOperation" {
	// XPECT warnings --> "Tests must send at least one message to assert object" at "1 + 1"
	1 + 1
}

test "okTest" {
	assert.equals(1, 1.0)
}

test "testForVariable" {
	// XPECT warnings --> "Tests must send at least one message to assert object" at "assert"
	assert
}

test "testForBinaryOperationForAssert" {
	// XPECT warnings --> "Tests must send at least one message to assert object" at "assert + 1"
	assert + 1
}

test "testTryWithAssert" {
	try {
		assert.equals(1, 1)
		return 1 / 0
	} catch e : Exception {
		
	}
}

test "testOkCatchWithAssert" {
	try {
		1 / 0
	} catch e : Exception {
		assert.equals(1, 1)
	}
}

test "testBadTryCatchWithAssert" {
	// XPECT warnings --> "Tests must send at least one message to assert object" at "try { 		1 / 0 	} catch e : Exception { 		1 + 1 	}"
	try {
		1 / 0
	} catch e : Exception {
		1 + 1
	}
}
